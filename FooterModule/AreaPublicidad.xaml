<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="Decktra.PubliPuntoEstacion.FooterModule.AreaPublicidad"
	x:Name="UserControl"
	d:DesignWidth="640" d:DesignHeight="480" Width="960" Height="171">

    <Grid x:Name="LayoutRoot" Background="{DynamicResource BackgroundColorBrush}">
        <StackPanel Background="{DynamicResource ColorHeaderBrush}">
			<TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" FontSize="32" Margin="0,55,0,0" Style="{DynamicResource PubliPuntoContentHeaderText}">
                <Run Language="es-ve" Text="PUBLICIDAD AQUI"/></TextBlock>
		</StackPanel>
	</Grid>
</UserControl>