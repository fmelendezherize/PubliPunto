<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="Decktra.PubliPuntoEstacion.FooterModule.AreaPublicidad"
	x:Name="UserControl"
	d:DesignWidth="640" d:DesignHeight="480" Width="960" Height="171">

    <Grid x:Name="LayoutRoot" Background="{DynamicResource BackgroundColorBrush}">
        <StackPanel MouseUp="StackPanel_MouseUp">
        	<StackPanel.Background>
        		<ImageBrush ImageSource="Resources/Banner_Anuncia_Aqui.png"/>
        	</StackPanel.Background>
		</StackPanel>
	</Grid>
</UserControl>