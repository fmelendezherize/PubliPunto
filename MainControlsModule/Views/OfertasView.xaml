<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:local="clr-namespace:Decktra.PubliPuntoEstacion.MainControlsModule"
	x:Class="Decktra.PubliPuntoEstacion.MainControlsModule.OfertasView"
	d:DesignWidth="640" d:DesignHeight="480"
	Width="970" Height="294">

	<Grid x:Name="LayoutRoot" Background="{DynamicResource BackgroundColorBrush}">
		<DockPanel>
            <StackPanel DockPanel.Dock="Top" Style="{DynamicResource PubliPuntoContentHeader}">
                <TextBlock Margin="60,5,0,0" Style="{DynamicResource PubliPuntoContentHeaderText}">Ofertas</TextBlock>
            </StackPanel>
            <StackPanel Background="White" DockPanel.Dock="Bottom" VerticalAlignment="Stretch" Height="247">
                <TextBlock></TextBlock>
            </StackPanel>
        </DockPanel>
	</Grid>
</UserControl>
