<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:local="clr-namespace:Decktra.PubliPuntoEstacion.MainControlsModule"
	xmlns:views="clr-namespace:Decktra.PubliPuntoEstacion.MainControlsModule.Views"
	mc:Ignorable="d"
	x:Class="Decktra.PubliPuntoEstacion.MainControlsModule.Views.HomeControlsView"
	x:Name="UserControl">
	<Grid x:Name="LayoutRoot" Style="{DynamicResource HomeControlStyle}">
		<StackPanel>
			<views:NuestrosClientesView x:Name="NuestrosClientesView"/>
			<views:OfertasView x:Name="OfertasView" Margin="0,30,0,0" />
		</StackPanel>
	</Grid>
</UserControl>